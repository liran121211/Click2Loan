<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>

<Pane prefHeight="640.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.LoanController">
   <children>
      <Accordion layoutX="52.0" layoutY="111.0" />
      <Label layoutX="78.0" layoutY="39.0" opacity="0.55" text="Terms of Use Agreement" textAlignment="CENTER">
         <font>
            <Font name="Arial" size="29.0" />
         </font>
         <effect>
            <Glow />
         </effect>
      </Label>
      <Button fx:id="nextBtn" defaultButton="true" layoutX="349.0" layoutY="603.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="113.0" text="Accept">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
      </Button>
      <Button fx:id="backBtn" defaultButton="true" layoutX="19.0" layoutY="603.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="113.0" text="Reject">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
      </Button>
      <WebView fx:id="agreement" layoutX="27.0" layoutY="90.0" prefHeight="484.0" prefWidth="426.0" />
      <Separator layoutX="71.0" layoutY="71.0" prefHeight="0.0" prefWidth="347.0" />
   </children>
</Pane>
